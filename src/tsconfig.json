{
  "compilerOptions": {
    "lib": [
      "es2018",
      "esnext.asynciterable"
    ],
    "baseUrl": "./",
    "outDir": "../build",
    "paths": {
      "@prisma/*": [
        "../database/node_modules/@prisma/*"
      ],
      "@shared_libs/*": [
        "./create_prisma_client/nodejs/node_modules/@shared_libs/*"
      ],
      "uuid": [
        "./src/external_libraries/nodejs/node_modules/uuid"
      ],
    },
    "target": "es2018",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "include": [
    "create_prisma_client/nodejs/node_modules/@shared_libs",
    "external_libraries/nodejs/node_modules/**/*",
    "lambdas"
  ]
}